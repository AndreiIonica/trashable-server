{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "City Type",
  "description": "The city object schema",
  "default": {},
  "required": ["name", "county_id"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database id"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name of the city.Must be unique",
      "examples": ["Arges"]
    },
    "county_id": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The id of the county",
      "examples": ["Arges"]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The date the city was created"
    },
    "update_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the city was last updated"
    },
    "deleted_at": {
      "$id": "#/properties/deleted_at",
      "type": "string",
      "title": "The date the city was deleted"
    }
  },
  "additionalProperties": false
}
